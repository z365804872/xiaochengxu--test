<!--pages/evaluate/index/index.wxml-->
<view scroll-y class="container">
    <view class="appra-one">
        <view class="appra-one-title">
            品牌选择
        </view>
        <view class="appra-one-box">
            <view  wx:for="{{dataList}}" wx:key="{{index}}" class="nav-item {{index==choose?'select':''}}" bindtap="changeTab" data-index="{{index}}" data-brandId="{{item.brandId}}">
                <view  style="background:url('{{item.photo}}'); background-size:120rpx 120rpx;background-repeat:no-repeat;background-position:48rpx 48rpx;position: absolute;width:216rpx;height:216rpx;left:0;top:0;">
                    <!-- <text class="item-text">{{item.brandName}}</text> -->
                </view>
                
            </view>
        </view>
        <view class="next-btn"  bindtap="nextTab">
            下一步(1/2)
        </view>
    </view>
</view>