/**index.wxss**/
.container{
  background: #fff;
  padding-bottom: 150rpx;
}
.section-swiper .slide{
  width: 100%;
  height: 400rpx;
}
.section-swiper .slide-image{
  width: 100%;
  height: 400rpx;
  display: block;
}

.detail-header{
  height: 254rpx;
  width: 750rpx;
}
.detail-header-name{
  height: 254rpx;
  width: 455rpx;
  float: left
}
.header-name{
  display: block;
  font-size: 44rpx;
  color: #333333;
  margin-left: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-top: 30rpx;
}
.header-info{
  display: block;
  opacity: 0.5;
  font-family: PingFangSC-Light;
  margin-left: 30rpx;
  margin-top: 50rpx;
  font-size: 24rpx;
  color: #000000;
}

.detail-header-price{
  height: 254rpx;
  width: 264rpx;
  float: right;
  text-align: right;
  padding-right: 30rpx;
}
.header-price{
  font-family: PingFangSC-Semibold;
  font-size: 44rpx;
  color: #333333;
  margin-top: 30rpx;
}
.header-priceIcon{
  font-family: PingFangSC-Semibold;
  font-size: 24rpx;
  background: #000;
  width: 140rpx;
  text-align: center;
  display: inline-block;
  line-height: 40rpx;
  height: 40rpx;
  color: #FFFFFF;
  margin-top: 20rpx;
}
.header-fprice{
  font-family: PingFangSC-Light;
  font-size: 24rpx;
  color: #000000;
  margin-top: 45rpx;
}
.calendar{
  width: 750rpx;
  height: 330rpx;
  background: red;
}


.sizeList{
  width: 750rpx;
  border-top: 10rpx solid #f7f7f7;
  padding-bottom: 30rpx;
  border-bottom: 10rpx solid #f7f7f7;
}
.sizeList-box{
  overflow:hidden
}
.sizeList-title{
  opacity: 0.9;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #333333;
  margin-left: 30rpx;
  margin-top: 10rpx;
  line-height: 60rpx;
}
.sizeList-item{
  width: 128rpx;
  height: 86rpx;
  border: 1rpx solid #ccc;
  text-align: center;
  opacity: 0.6;
  font-family: PingFangSC-Light;
  color: #333333;
  margin: 5rpx;
  float: left;
}
.sizeList-item:nth-child(5n+1){
  margin-left: 30rpx;
}
.sizeList-size{
  margin-top: 3rpx;
  line-height: 50rpx;
  font-size: 28rpx;

}
.sizeList-price{
  font-size: 20rpx;
}
.size-choode{
  background: #5DBCFF;
  color: #fff;
  border: 1rpx solid #5DBCFF;

}

.sellList{
  width: 750rpx;
}
.sellList-tab{
  overflow: hidden;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.sellList-tab view{
  width: 152rpx;
  height: 60rpx;
  background: #f5f5f5;
  border-radius: 200rpx;
  line-height: 60rpx;
  text-align: center;
  opacity: 0.8;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  float: left;
}
.sellList-tab-cj{
  margin-left: 30rpx;
}
.sellList-tab-cs{
  margin-left: 40rpx;
}
.sellList-tab-qg{
  margin-left: 40rpx;
}

.sellList-tab .sellList-tab-active{
  background: #082359;
  color:#fff;
}
.sellList-num{
  margin-left: 30rpx;
  margin-bottom: 30rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
}

.box-cj-item{
  background: #FFFFFF;
  box-shadow: 0 2rpx 12rpx 0 rgba(0,0,0,0.10);
  border-radius: 8rpx;
  padding: 0 30rpx;
  margin: 0 30rpx 30rpx;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cj-item-size{
  float: left;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #333333;
  line-height: 120rpx;
  width: 120rpx;
  text-align: center;
  height: 120rpx;
  margin-left:8rpx;
}
.cj-item-date{
  float: left;
  opacity: 0.6;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #333333;
  line-height: 120rpx;
  height: 120rpx;
}
.cj-item-price{
  width: 112rpx;
  height: 42rpx;
  border-radius: 200rpx;
  text-align: center;
  line-height: 42rpx;
}
.cj-item-info{
  float: right;
  margin-right: 30rpx;
  margin-left: 20rpx;
  width: 112rpx;
  height: 42rpx;
  line-height: 42rpx;
  background: #47C99D;
  border-radius: 200rpx;
  color: #fff;
  font-family: PingFangSC-Semibold;
  font-size: 24rpx;
  text-align: center;
  color: #FFFFFF;
  margin-top: 40rpx;
}

.addmore-btn{
  background: #FFFFFF;
  box-shadow: 0 2rpx 12rpx 0 rgba(0,0,0,0.10);
  border-radius: 8rpx;
  margin-left: 30rpx;
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #333333;
}

.detail-buttom{
  width: 750rpx;
  height: 120rpx;
  background: #FFFFFF;
  box-shadow: 0 0 8rpx 0 rgba(0,0,0,0.20);
  position: fixed;
  bottom:0;
}
.detail-buttom-price{
  font-family: PingFangSC-Semibold;
  font-size: 44rpx;
  color: #000000;
  line-height: 120rpx;
  margin-left: 30rpx;
  float: left;
}
.detail-buttom-sell{
  float: right;
  width:160rpx;
  height: 80rpx;
  background: #47C99D;
  line-height: 80rpx;
  text-align: center;
  font-family: PingFangSC-Semibold;
  font-size: 32rpx;
  color: #FFFFFF;
  margin-top: 20rpx;
  margin-right: 20rpx;
}
.detail-buttom-buy{
  float: right;
  width:160rpx;
  height: 80rpx;
  background: #FF484A;
  line-height: 80rpx;
  text-align: center;
  font-family: PingFangSC-Semibold;
  font-size: 32rpx;
  color: #FFFFFF;
  margin-top: 20rpx;
  margin-right: 30rpx;
}
.sellList-box-no{
  width: 750rpx;
  height: 670rpx;
  overflow: hidden;
}
.sellList-box-no view{
  width: 750rpx;
  margin-top: 320rpx;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
}
.detail-buttom-info{
  margin-left: 30rpx;
  float: left;
  opacity: 0.4;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #000000;
  display: block;
  margin-top:-10px;
}

.list-btn{
  display: block;
  width: 100%;
  padding: 0;
  margin: 0;
  background-color: #fff;
  /* box-shadow: 0 2rpx 12rpx 0 rgba(0,0,0,0.10); */
}

.list-btn::after{
  border: 0;
}