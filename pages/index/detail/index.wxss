/**index.wxss**/
.container{
  background: #fff;
}
.section-swiper .slide{
  width: 100%;
  height: 400rpx;
}
.section-swiper .slide-image{
  width: 100%;
  height: 400rpx;
  display: block;
}

.detail-header{
  height: 254rpx;
  width: 750rpx;
}
.detail-header-name{
  height: 254rpx;
  width: 465rpx;
  float: left
}
.header-name{
  display: block;
  font-size: 44rpx;
  color: #333333;
  margin-left: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  margin-top: 30rpx;
}
.header-info{
  display: block;
  opacity: 0.5;
  font-family: PingFangSC-Light;
  margin-left: 30rpx;
  margin-top: 50rpx;
  font-size: 24rpx;
  color: #000000;
}

.detail-header-price{
  height: 254rpx;
  width: 254rpx;
  float: right;
  text-align: right;
  padding-right: 30rpx;
}
.header-price{
  font-family: PingFangSC-Semibold;
  font-size: 44rpx;
  color: #333333;
  margin-top: 30rpx;
}
.header-priceIcon{
  font-family: PingFangSC-Semibold;
  font-size: 24rpx;
  background: #000;
  width: 140rpx;
  text-align: center;
  display: inline-block;
  line-height: 40rpx;
  height: 40rpx;
  color: #FFFFFF;
  margin-top: 20rpx;
}
.header-fprice{
  font-family: PingFangSC-Light;
  font-size: 24rpx;
  color: #000000;
  margin-top: 45rpx;
}
.calendar{
  width: 750rpx;
  height: 330rpx;
  background: red;
}


.sizeList{
  width: 750rpx;
  border-top: 10rpx solid #f7f7f7;
  padding-bottom: 30rpx;
  border-bottom: 10rpx solid #f7f7f7;
}
.sizeList-box{
  overflow:hidden
}
.sizeList-title{
  opacity: 0.9;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #333333;
  margin-left: 30rpx;
  margin-top: 10rpx;
  line-height: 60rpx;
}
.sizeList-item{
  width: 128rpx;
  height: 86rpx;
  border: 1rpx solid #ccc;
  text-align: center;
  opacity: 0.6;
  font-family: PingFangSC-Light;
  color: #333333;
  margin: 5rpx;
  float: left;
}
.sizeList-item:nth-child(5n+1){
  margin-left: 30rpx;
}
.sizeList-size{
  margin-top: 3rpx;
  line-height: 50rpx;
  font-size: 28rpx;

}
.sizeList-price{
  font-size: 20rpx;
}
.size-choode{
  background: #5DBCFF;
  color: #fff;
  border: 1rpx solid #5DBCFF;

}

.sellList{
  width: 750rpx;
}
.sellList-tab{
  overflow: hidden;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.sellList-tab view{
  width: 152rpx;
  height: 60rpx;
  background: #f5f5f5;
  border-radius: 200rpx;
  line-height: 60rpx;
  text-align: center;
  opacity: 0.8;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  float: left;
}
.sellList-tab-cj{
  margin-left: 30rpx;
}
.sellList-tab-cs{
  margin-left: 40rpx;
}
.sellList-tab-qg{
  margin-left: 40rpx;
}